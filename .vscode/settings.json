{
    "protoc": {
        "path": "/usr/local/bin/protoc",
        "options": [
            "-I=${workspaceRoot}/YarnSpinner", 
            "--csharp_out=${workspaceRoot}/YarnSpinner", 
            "${workspaceRoot}/YarnSpinner/yarn_spinner.proto"
        ]
    },
    "antlr4.generation": {
        "mode": "external",
        "language": "CSharp",
        "listeners": true,
        "visitors": true,
        "outputDir": "."
    }
}
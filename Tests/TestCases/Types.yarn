// Testing types

// string equality
<<assert("test" == "test")>>

// string-number inequality
<<assert("test" != 1)>>

// string case sensitivity
<<assert("hi" != "HI")>>

// multiple-type concatenation
<<assert(1+1+"hi" == "2hi")>>

// gt test
<<assert(3 gt 2)>>

// lt test
<<assert(1 lt 2)>>

// lte test
<<assert(2 lte 2)>>

// gte test
<<assert(2 gte 2)>>
